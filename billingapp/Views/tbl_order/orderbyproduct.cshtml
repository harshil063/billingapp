@model IEnumerable<billingapp.tbl_order>

@{
    ViewBag.Title = "orderbyproduct";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>orderbyproduct</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Product name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.prod_qty)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.tbl_product.product_name)
            </td>

            <td>
                @Html.ActionLink(" view List", "listorderbyproduct", new { id = item.product_id })

            </td>
        </tr>
    }

</table>
